<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Article.
    -->
    <changeSet id="20180720045233-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="category_id"
                                 baseTableName="article"
                                 constraintName="fk_article_category_id"
                                 referencedColumnNames="id"
                                 referencedTableName="support_category"/>

        <addForeignKeyConstraint baseColumnNames="user_id"
                                 baseTableName="article"
                                 constraintName="fk_article_user_id"
                                 referencedColumnNames="id"
                                 referencedTableName="rev_user"/>

    </changeSet>
</databaseChangeLog>
